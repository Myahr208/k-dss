{
  "name": "k-dss",
  "url": "https://github.com/dapphub/k-dss/",
  "src": {
    "specification": "./src/dss.md",
    "smt_prelude": "./src/prelude.smt2.md",
    "rules": [
      "./src/storage.k.md",
      "./src/lemmas.k.md"
    ]
  },
  "implementations": {
    "Vat": {
      "solc_output": "./dss/out/vat.sol.json"
    },
    "Dai": {
      "solc_output": "./dss/out/dai.sol.json"
    },
    "Jug": {
      "solc_output": "./dss/out/jug.sol.json"
    },
    "Vow": {
      "solc_output": "./dss/out/vow.sol.json"
    },
    "Cat": {
      "solc_output": "./dss/out/cat.sol.json"
    },
    "GemJoin": {
      "solc_output": "./dss/out/join.sol.json"
    },
    "DaiJoin": {
      "solc_output": "./dss/out/join.sol.json"
    },
    "VatLike": {
      "name" : "Vat",
      "solc_output": "./dss/out/vat.sol.json"
    },
    "VowLike": {
      "name" : "Vow",
      "solc_output": "./dss/out/vow.sol.json"
    },
    "Flipper": {
      "name" : "Flipper",
      "solc_output": "./dss/out/flip.sol.json"
    },
    "Flopper": {
      "name" : "Flopper",
      "solc_output": "./dss/out/flop.sol.json"
    },
    "Flapper": {
      "name" : "Flapper",
      "solc_output": "./dss/out/flap.sol.json"
    },
    "GemLike": {
      "name" : "DSToken",
      "solc_output": "./dss/out/test/vat.t.sol.json"
    },
    "Gemish": {
      "name" : "DSTokenBase",
      "solc_output": "./dss/out/test/vat.t.sol.json"
    },
    "End": {
      "solc_output": "./dss/out/end.sol.json"
    }
  },
  "split-fail":[
    "Vat_heal",
    "Vat_grab",
    "Vat_fold",
    "Vat_frob",
    "Vat_fork"
    ],
  "dapp_root": "./dss",
  "host": "127.0.0.1:8080"
}
