requires "../rules.k"

module PROOF-DAIMOVE_MOVE_FAIL
  imports ETHEREUM-SIMULATION
  imports EVM
  imports RULES

// DaiMove_move
rule
  <k> #execute ~> CONTINUATION => #halt ~> CONTINUATION </k>
  <exit-code> 1 </exit-code>
  <mode> NORMAL </mode>
  <schedule> BYZANTIUM </schedule>
  <analysis> .Map </analysis>
  <ethereum>
    <evm>
      <output> _ => _ </output>
      <statusCode> _ => EVMC_REVERT </statusCode>
      <callStack> VCallStack </callStack>
      <interimStates> _ </interimStates>
      <touchedAccounts> _ => _ </touchedAccounts>
      <callState>
        <program> #asMapOpCodes(#dasmOpCodes(#parseByteStack(""), BYZANTIUM)) </program>
        <programBytes> #parseByteStack("") </programBytes>
        <id> ACCT_ID </id>
        <caller> CALLER_ID </caller>
        <callData> #abiCallData("move", #address(ABI_src), #address(ABI_dst), #uint256(ABI_wad)) </callData>
        <callValue> 0 </callValue>
        <wordStack> .WordStack => _ </wordStack>
        <localMem> .Map => _ </localMem>
        <pc> 0 => _ </pc>
        <gas> VGas => _ </gas>
        <memoryUsed> 0 => _ </memoryUsed>
        <previousGas> _ => _ </previousGas>
        <static> false </static>
        <callDepth> VCallDepth => _ </callDepth>
      </callState>
      <substate>
        <selfDestruct> VSelfDestruct </selfDestruct>
        <log> _ => VLog </log>
        <refund> _ => VRefund </refund>
      </substate>
      <gasPrice> _ </gasPrice>
      <origin> ORIGIN_ID </origin>
      <previousHash> _ </previousHash>
      <ommersHash> _ </ommersHash>
      <coinbase> _ </coinbase>
      <stateRoot> _ </stateRoot>
      <transactionsRoot> _ </transactionsRoot>
      <receiptsRoot> _ </receiptsRoot>
      <logsBloom> _ </logsBloom>
      <difficulty> _ </difficulty>
      <number> _ </number>
      <gasLimit> _ </gasLimit>
      <gasUsed> _ </gasUsed>
      <timestamp> TIME </timestamp>
      <extraData> _ </extraData>
      <mixHash> _ </mixHash>
      <blockNonce> _ </blockNonce>
      <ommerBlockHeaders> _ </ommerBlockHeaders>
      <blockhash> _ </blockhash>
    </evm>
    <network>
      <activeAccounts> SetItem(ACCT_ID)
      SetItem(Vat) _ </activeAccounts>
      <accounts>
        <account>
          <acctID> Vat </acctID>
          <balance> Vat_balance => _ </balance>
          <code> #parseByteStack("") </code>
          <storage> (_:Map
                      (#Vat.dai[ABI_dst] |-> (Dai_dst => _)
                      (#Vat.dai[ABI_src] |-> (Dai_src => _)
                      (#Vat.wards[ACCT_ID] |-> (Can_move))))) </storage>
          <nonce> _ </nonce>
        </account>
        <account>
          <acctID> ACCT_ID </acctID>
          <balance> BAL </balance>
          <code> #parseByteStack("") </code>
          <storage> (_:Map
                      (#DaiMove.vat |-> (Vat)
                      (#DaiMove.can[ABI_src][CALLER_ID] |-> (Can))))
           </storage>
          <nonce> _ </nonce>
        </account>
       ... 
      </accounts>
      <txOrder> _ </txOrder>
      <txPending> _ </txPending>
      <messages> _ </messages>
    </network>
  </ethereum>
requires #rangeAddress(ACCT_ID)
andBool #notPrecompileAddress(ACCT_ID)
andBool #rangeAddress(CALLER_ID)
andBool #rangeAddress(ORIGIN_ID)
andBool #rangeUInt(48, TIME)
andBool #rangeUInt(256, BAL)
andBool VCallDepth <=Int 1024
  andBool #rangeAddress(ABI_src)
  andBool #rangeAddress(ABI_dst)
  andBool #rangeUInt(256, ABI_wad)
  andBool #rangeUInt(256, Can)
  andBool #rangeAddress(Vat)
  andBool #rangeUInt(256, Can_move)
  andBool #rangeUInt(256, Dai_src)
  andBool #rangeUInt(256, Dai_dst)
  andBool #rangeUInt(256, Vat_balance)
  andBool #notPrecompileAddress(Vat)
  andBool ACCT_ID =/=Int Vat
  andBool VGas >Int 300000
  andBool notBool (
            (#rangeSInt(256, #Ray *Int ABI_wad)
    andBool (#rangeUInt(256, Dai_dst +Int #Ray *Int ABI_wad)
    andBool (#rangeUInt(256, Dai_src -Int #Ray *Int ABI_wad)
    andBool (Can_move ==Int 1
    andBool (VCallDepth <Int 1024
    andBool (((ABI_src ==Int CALLER_ID) orBool (Can ==Int 1))))))))
  )

endmodule
