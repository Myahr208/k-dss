behaviour file-Line of Pit
interface file(bytes32 what, uint256 data)

types

    Can  : uint256
    Line : uint256

storage

    wards[CALLER_ID] |-> Can
    Line             |-> Line => #if what == #string2Word("Line") #then data #else Line #fi

iff

    // act: caller is `. ? : not` authorised
    Can == 1

if

    VGas > 300000