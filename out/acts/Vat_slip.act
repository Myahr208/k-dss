behaviour slip of Vat
interface slip(bytes32 ilk, bytes32 guy, int256 wad)

types

    Can : uint256
    Gem : uint256

storage

    wards[CALLER_ID] |-> Can
    gem[ilk][guy]    |-> Gem => Gem + wad

iff

    // act: caller is `. ? : not` authorised
    Can == 1

iff in range uint256

    Gem + wad

if

    VGas > 300000