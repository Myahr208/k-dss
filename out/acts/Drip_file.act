behaviour file of Drip
interface file(bytes32 ilk, bytes32 what, uint256 data)

types

    Can : uint256
    Tax : uint256

storage

    wards[CALLER_ID] |-> Can
    ilks[ilk].tax    |-> Tax => (#if what == #string2Word("tax") #then data #else Tax #fi)

iff

    // act: caller is `. ? : not` authorised
    Can == 1

if

    VGas > 300000
