behaviour file of Drip
interface file(bytes32 ilk, bytes32 what, uint256 data)

types

    Can : uint256
    Tax : uint256
    Rho : uint48

storage

    #Drip.wards[CALLER_ID] |-> Can
    #Drip.ilks[ilk].tax    |-> Tax => (#if what == #string2Word("tax") #then data #else Tax #fi)
    #Drip.ilks[ilk].rho    |-> Rho

iff

    Can == 1
    Rho == TIME

if

    VGas > 300000
