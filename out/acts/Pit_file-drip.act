behaviour file-drip of Pit
interface file(bytes32 what, address data)

types

    Can    : uint256
    Drip   : address

storage

    #Pit.wards[CALLER_ID] |-> Can
    #Pit.drip             |-> Drip => (#if what == #string2Word("drip") #then data #else Drip #fi)

iff

    Can == 1

if

    VGas > 300000
