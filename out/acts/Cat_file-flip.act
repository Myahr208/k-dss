behaviour file-flip of Cat
interface file(bytes32 ilk, bytes32 what, address data)

types

    Can  : uint256
    Flip : address

storage

    wards[CALLER_ID] |-> Can
    ilks[ilk].flip   |-> Flip => (#if what == #string2Word("flip") #then data #else Flip #fi)

iff

    // act: caller is `. ? : not` authorised
    Can == 1

if

    VGas > 300000
