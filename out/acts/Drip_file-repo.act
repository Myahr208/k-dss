behaviour file-repo of Drip
interface file(bytes32 what, uint256 data)

types

    Can  : uint256
    Repo : uint256

storage

    #Drip.wards[CALLER_ID] |-> Can
    #Drip.repo             |-> Repo => (#if what == #string2Word("repo") #then data #else Repo #fi)

iff

    Can == 1

if

    VGas > 300000
