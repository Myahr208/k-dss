behaviour flux of Vat
interface flux(bytes32 ilk, bytes32 src, bytes32 dst, int256 wad)

types

    Can     : uint256
    Gem_src : uint256
    Gem_dst : uint256

storage

    wards[CALLER_ID] |-> Can
    gem[ilk][src]    |-> Gem_src => Gem_src - wad
    gem[ilk][dst]    |-> Gem_dst => Gem_dst + wad

iff

    // act: caller is `. ? : not` authorised
    Can == 1

iff in range uint256

    Gem_src - wad
    Gem_dst + wad

if

    VGas > 300000