behaviour init of Drip
interface init(bytes32 ilk)

types

    Can : uint256
    Tax : uint256
    Rho : uint48

storage

    wards[CALLER_ID] |-> Can
    ilks[ilk].tax    |-> Tax => #Ray
    ilks[ilk].rho    |-> Rho => TIME

iff

    // act: caller is `. ? : not` authorised
    Can == 1
    // act: `Tax` is `. ? : not` zero
    Tax == 0

if

    VGas > 300000