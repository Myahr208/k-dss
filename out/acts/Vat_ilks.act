behaviour ilks of Vat
interface ilks(bytes32 ilk)

types

    Take  : uint256
    Rate  : uint256
    Ink_i : uint256
    Art_i : uint256

storage

    ilks[ilk].take |-> Take
    ilks[ilk].rate |-> Rate
    ilks[ilk].Ink  |-> Ink_i
    ilks[ilk].Art  |-> Art_i

if

    VGas > 300000

returns Take : Rate : Ink_i : Art_i